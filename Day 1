1//Declaring integer and display

import java.util.Scanner;
public class Main
{
    public static void main(String[]args)
    {
        int num=20;
        System.out.print("My Age is"+num);
    }
}

 2//Adding two numbers 
import java.util.Scanner;
public class Main{
    public static void main(String[]args)
    {
        int num1=5;
        int num2=5;
       System.out.println(num1+num2);
    }
}

3//Getting user input and addition of two integer number
import java.util.Scanner;
public class Main{
    public static void main(String[]args)
    
    {
        int num1,num2;
        
        Scanner hit=new Scanner(System.in);
        num1=hit.nextInt();
        num2=hit.nextInt();
        System.out.print(num1+num2);
    }
}

4//Getting user input and addition of two float number
import java.util.Scanner;
public class Main{
    public static void main(String[]args)
    {
        Float num1,num2;
        Scanner hit=new Scanner(System.in);
        num1=hit.nextFloat();
        num2=hit.nextFloat();
       System.out.println(num1+num2);
        
    }
}
